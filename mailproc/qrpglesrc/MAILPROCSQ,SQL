CREATE TABLE alan/MAILPROCS
  (EXTENSION CHAR ( 4) NOT NULL WITH DEFAULT,
   MIMETYPE DEC ( 5, 0) NOT NULL WITH DEFAULT,
   FILEDESC CHAR ( 50) NOT NULL WITH DEFAULT,
   PRIMARY KEY (EXTENSION));

